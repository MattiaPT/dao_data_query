well.\n\n---\nConfig Hash: `21dcbc825a47e5e7da0bd9f8e4d318034f4409dff4349d78df3463e916c11992`\n<details> <summary>Proposal Config</summary> <pre>{\n  \"_meta\": {\n    \"generatedAt\": \"2023-02-17T20:33:40.410Z\",\n    \"generatorVersion\": \"1.0.0\"\n  },\n  \"snapshotBlock\": 2970608,\n  \"safetyModuleInfo\": {\n    \"totalStaked\": \"153019927.875422612166240225\",\n    \"stakedTVL\": \"685120.7840679998108574456047975558423303764686\",\n    \"govTokenPrice\": \"0.004477330460028539185016\",\n    \"emissions\": {\n      \"emissionsPerSecond\": \"2.50400641025641\",\n      \"lastUpdateTimestamp\": 1676665482,\n      \"emissionAPR\": \"51.640615565209398183\",\n      \"emissionsPerYear\": \"79020432.692307684216\"\n    }\n  },\n  \"dexInfo\": {\n    \"govTokenTotal\": \"44793971.627716713566729824\",\n    \"nativeAssetTotal\": \"414032.6457358179299795\",\n    \"govTokenPrice\": \"0.004477330460028539185016\",\n    \"nextFreeSlot\": 10,\n    \"emissionsPerYear\": \"52680288.461538561336\",\n    \"poolTVL\": \"401114.827188860410629369345547755419709221117184\",\n    \"currentPoolAPR\": \"58.802877426638042444\",\n    \"currentPoolRewardInfo\": {\n      \"startTimestamp\": 1674790200,\n      \"endTimestamp\": 1677209400,\n      \"rewardPerSec\": \"1.66933760683761\"\n    }\n  },\n  \"marketData\": {\n    \"assets\": {\n      \"GLMR\": {\n        \"price\": \"0.4844\",\n        \"totalSuppliedUnderlying\": \"21538581.80091318446886176596330300285147776\",\n        \"totalSuppliedTVL\": \"10433289.024362346556716639432623974581255826944\",\n        \"totalBorrows\": \"11666908.159047412358869696\",\n        \"totalBorrowedTVL\": \"5651450.3122425665466364807424\",\n        \"utilization\": \"0.54167485430970962511\",\n        \"govSupplySpeed\": \"447144001831502000\",\n        \"govBorrowSpeed\": \"1\",\n        \"nativeSupplySpeed\": \"51669973544973600\",\n        \"nativeBorrowSpeed\": \"1\",\n        \"nativeSupplyBorrowSplit\": {\n          \"SUPPLY\": 1,\n    